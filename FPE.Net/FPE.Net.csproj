<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net5.0</TargetFramework>
    <Version>1.0.0.0</Version>
    <AssemblyVersion>1.0.0.0</AssemblyVersion>
    <FileVersion>1.0.0.0</FileVersion>
    <Authors>Matthias Hunstock</Authors>
    <Product>FPE.Net</Product>
    <PackageId>FPE.Net</PackageId>
    <NeutralLanguage>en</NeutralLanguage>
    <Description>Format-Preserving-Encryption for .NET</Description>
    <Copyright>see README</Copyright>
    <PackageLicenseUrl>http://www.apache.org/licenses/LICENSE-2.0</PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/a-tze/FPE.Net</PackageProjectUrl>
    <PackageIconUrl>Icon URL</PackageIconUrl>
    <RepositoryUrl>https://github.com/a-tze/FPE.Net</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageTags>Tags</PackageTags>
    <PackageReleaseNotes>Release</PackageReleaseNotes>
  </PropertyGroup>

  <ItemGroup>
    <AssemblyAttribute Include="System.Runtime.CompilerServices.InternalsVisibleTo">
        <_Parameter1>FPE.Net.Test</_Parameter1>
    </AssemblyAttribute>
